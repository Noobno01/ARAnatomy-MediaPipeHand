  m_Script: {fileID: 11500000, guid: 174bdc12c25748c489c4d5fa2f6c94a6, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  descriptionText: "\n\nTAENIA COLI\n\n\nThe taeniae coli (also teniae coli or tenia
    coli) are three separate longitudinal ribbons (taeniae meaning ribbon in Latin)
